<mapper namespace="com.shiny.ucenter.dao.PermissionFileMapper">
  <resultMap id="BaseResultMap" type="com.shiny.ucenter.entity.PermissionFileKey">
    <id column="permission_id" jdbcType="VARCHAR" property="permissionId" />
