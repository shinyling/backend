    delete from permission_file
    where permission_id = #{permissionId,jdbcType=VARCHAR}
      and file_id = #{fileId,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="com.shiny.ucenter.entity.PermissionFileKey">
    insert into permission_file (permission_id, file_id)
    values (#{permissionId,jdbcType=VARCHAR}, #{fileId,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.shiny.ucenter.entity.PermissionFileKey">
    insert into permission_file
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="permissionId != null">
        permission_id,
      </if>
      <if test="fileId != null">
        file_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
